<div class='container'>
  <div class='logo2'>
    <%= image_tag('logo2.png') %>
  </div>
  <h5><strong><u>Topics That Students are Interested in Learning More About</u></strong></h5>
  <% @topics.each do |topic| %>
  <div class='indextopics'>
    <h2><%= link_to topic.title, topic_path(topic) %></h2>
  </div>
  <div class='divider'>
    <%= image_tag('divider.png') %>
  </div>
  <% end %>
</div>
